{
  "name": "streamx/magento-connector-test-tools",
  "description": "Test tools for integration tests of StreamX Connector with Magento",
  "version": "1.0.0",
  "type": "magento2-component",
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "magento/module-catalog": ">=102.0.0",
    "ext-xdebug": "*"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "StreamX\\ConnectorTestTools\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true
    }
  }
}
